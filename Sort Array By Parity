class Solution {
    public int[] sortArrayByParity(int[] nums) {
        int n=nums.length;
        int a[]=new int[n];
        int j=n-1;
        int k=0;
        for(int i=0;i<n;i++)
        {
            if(nums[i]%2==0)
              a[k++]=nums[i];
              else
              a[j--]=nums[i];
        }
        return a;
    }
}
