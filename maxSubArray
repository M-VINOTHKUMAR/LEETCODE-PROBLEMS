class Solution {
    public int maxSubArray(int[] nums) {
        int n=nums.length;
        int dp[]=new int[nums.length];
        dp[0]=nums[0];
        
        for(int i=1;i<n;i++)
        {
            dp[i]=Math.max(dp[i-1],0)+nums[i];
        }
        int m=Integer.MIN_VALUE;
        for(int i=0;i<n;i++)
        {
           m=Math.max(dp[i],m);
        }
        return m;
        
    }
}
